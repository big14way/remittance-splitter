# Deployment Account
# Your wallet private key (without 0x prefix)
# Get testnet CELO from https://faucet.celo.org
PRIVATE_KEY=your_private_key_here

# CeloScan API Key (for contract verification)
# Get from https://celoscan.io/myapikey
CELOSCAN_API_KEY=your_celoscan_api_key_here

# Gas Reporter Configuration (optional)
# Set to "true" to enable gas reporting during tests
REPORT_GAS=false

# CoinMarketCap API Key (optional, for gas reporter USD pricing)
# Get from https://coinmarketcap.com/api/
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here
